<Defs>	

  <!-- Level 1 -->

<VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_CleanSTNL</defName>
	<label>Clean</label>
	<description>Clean the target piece of apparel using a small amount of water from a distant ocean, will also clean filth in the area and put out fires.</description>
	<iconPath>UI_HydromancerSTNL/Abilities/Clean</iconPath>
	<abilityClass>VPE_Hydromancer.Ability_Clean</abilityClass>
	<chance>0</chance>
	<castTime>60</castTime>
	<range>15.5</range>
	<hasAoE>True</hasAoE>
	<radius>5.5</radius>
	<targetMode>Location</targetMode>
	<targetingParameters>
		<canTargetLocations>true</canTargetLocations>
		<canTargetSelf>false</canTargetSelf>
		<canTargetAnimals>false</canTargetAnimals>
		<canTargetPawns>false</canTargetPawns>
		<canTargetHumans>false</canTargetHumans>
		<canTargetItems>True</canTargetItems>
		<canTargetMechs>false</canTargetMechs>
	</targetingParameters>
		<targetingParametersForAoE>
		<canTargetLocations>true</canTargetLocations>
		<canTargetSelf>false</canTargetSelf>
		<canTargetAnimals>false</canTargetAnimals>
		<canTargetPawns>false</canTargetPawns>
		<canTargetHumans>false</canTargetHumans>
		<canTargetItems>true</canTargetItems>
		<canTargetMechs>false</canTargetMechs>
	</targetingParametersForAoE>
	<needsTicking>true</needsTicking>
	<castSound>VPE_Resurrect_Cast</castSound>
	<modExtensions>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
			<path>VPE_HydromancerSTNL</path>
			<level>1</level>
			<order>1</order>
			<psyfocusCost>0.03</psyfocusCost>
			<entropyGain>5</entropyGain>
		</li>
	</modExtensions>
</VEF.Abilities.AbilityDef>

 <VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_SaltWoundsSTNL</defName>
	<label>Salt Wounds</label>
	<description>Skip ocean water directly onto the target's bleeding wounds, causing a horrible amount of pain in the affected part.</description>
	<iconPath>UI_HydromancerSTNL/Abilities/Salt</iconPath>
	<abilityClass>VPE_Hydromancer.Ability_Saltwound</abilityClass>
	<castTime>60</castTime>
	<range>15.5</range>
	<durationTime>20000</durationTime>
	<targetMode>Pawn</targetMode>
	<targetingParameters>
		<canTargetLocations>false</canTargetLocations>
		<canTargetSelf>True</canTargetSelf>
		<canTargetAnimals>true</canTargetAnimals>
		<canTargetPawns>True</canTargetPawns>
		<canTargetHumans>True</canTargetHumans>
		<canTargetItems>True</canTargetItems>
		<canTargetMechs>True</canTargetMechs>
	</targetingParameters>
	<needsTicking>true</needsTicking>
	<castSound>PsycastPsychicPulse</castSound>
	<modExtensions>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
			<path>VPE_HydromancerSTNL</path>
			<level>1</level>
			<order>2</order>
			<psyfocusCost>0.04</psyfocusCost>
			<entropyGain>5</entropyGain>
			<prerequisites>
			<li>VPE_CleanSTNL</li>
			</prerequisites>
		</li>
<li Class="VEF.Abilities.AbilityExtension_EffecterOnTarget">
	<effecterDef>Watersplash_SaltWoundsSTNL</effecterDef>
	<maintainForTicks>60</maintainForTicks>
	<scale>1</scale>
</li>
	</modExtensions>
</VEF.Abilities.AbilityDef>


<!-- Level 2 -->

<VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_HydromancerHydroShieldSTNL</defName>
	<label>Hydro Shield</label>
	<description>Envelop the target with a shield made out of water that will prevent the target from catching fire for the duration of this ability.\n\nWill put out fire in a 2.5 cells radius.</description>
	<iconPath>UI_HydromancerSTNL/Abilities/Shield</iconPath>
	<abilityClass>VEF.Abilities.Ability_Blank</abilityClass>
	<castTime>120</castTime>
	<range>14.9</range>
	<radius>2.5</radius>
	<!-- <hasAoE>True</hasAoE> -->
    <durationTime>5000</durationTime>
    <durationTimeStatFactors>
      <PsychicSensitivity>1</PsychicSensitivity>
    </durationTimeStatFactors>
	<chance>0</chance>
	<targetMode>Pawn</targetMode>
	<targetingParameters>
		<canTargetLocations>false</canTargetLocations>
		<canTargetSelf>True</canTargetSelf>
		<canTargetAnimals>True</canTargetAnimals>
		<canTargetPawns>True</canTargetPawns>
		<canTargetHumans>True</canTargetHumans>
		<canTargetItems>false</canTargetItems>
		<canTargetMechs>True</canTargetMechs>
	</targetingParameters>
	<needsTicking>true</needsTicking>
	<castSound>PsycastPsychicEffect</castSound>
	<modExtensions>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
			<path>VPE_HydromancerSTNL</path>
			<level>2</level>
			<order>1</order>
			<psyfocusCost>0.10</psyfocusCost>
			<entropyGain>10</entropyGain>
			<prerequisites>
			<li>VPE_CleanSTNL</li>
			</prerequisites>
		</li>
		<li Class="VEF.Abilities.AbilityExtension_ExtraHediffs">
			<onTarget>
				<li>VPE_Hydromancer_HydroshieldSTNL</li>
			</onTarget>
		</li>
		<li Class="VEF.Abilities.AbilityExtension_EffecterOnTarget">
			<effecterDef>Watersplash_MidSTNL</effecterDef>
			<maintainForTicks>60</maintainForTicks>
			<scale>0.5</scale>
			<!-- <onCaster>true</onCaster> -->
		</li>
	</modExtensions>
</VEF.Abilities.AbilityDef>

